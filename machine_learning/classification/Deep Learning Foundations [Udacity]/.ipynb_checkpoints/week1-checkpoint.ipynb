   "source": [
    "import math\n",
    "import numpy as np\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def softmax(scores):\n",
    "    probs = [math.exp(x)/np.sum(np.exp(scores)) for x in scores]\n",
    "    return probs\n"
   ]
  },
